{
  "name": "Weather app",
  "description": "Weather app which shows current weather conditions at the user's location",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./16.png",
    "32": "./32.png",
    "48": "./48.png",
    "128": "./128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./16.png",
      "32": "./32.png",
      "48": "./48.png",
      "128": "./128.png"
    }
  },
  "permissions": ["storage", "geolocation", "activeTab", "scripting", "tabs", "notifications"]
}
